   "metadata": {},
   "outputs": [],
   "source": [
    "A = np. array([[0,2],[2,3]])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "35480c6a-636a-43a6-ac0b-3abed7247b44",
   "metadata": {},
   "outputs": [],
   "source": [
    "Q,R = qr(A)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "0f97dc48-93d8-4086-9790-f0772554e2b2",
